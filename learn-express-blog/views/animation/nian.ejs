<body>
    <div class="dragon-poplayer" id="dragon-poplayer">
        <div class="dragon-section dragon-ready-play" id="dragon-ready-play">
          <!-- is-animationed添加在上面 -->
            <div class="dragon-play">
                <div class="music">
                    <input type="checkbox" name="music" id="music-control" checked/>
                    <label for="music-control">声音</label>
                    <!-- label[for] + input "for" 属性可把 label 绑定到另外一个元素。请把 "for" 属性的值设置为相关元素的 id 属性的值-->
                </div>
                <div id="music">
                    <audio src="//gw.alicdn.com/tfscom/TB1Ydd2LpXXXXaUXFXXsKFbFXXX.mp3" loop="loop"></audio>
                </div>

            </div>
        </div>

        <!-- 第二场景-->
        <div class="dragon-section dragon-playing" id="dragon-playing">
            <div class="dragon-wrap">
                <div class="dragon-content">
                    <div class="dragon dragon1"></div>
                    <div class="dragon dragon2"></div>
                    <div class="dragon dragon3"></div>
                    <div class="dragon dragon4"></div>
                    <div class="dragon dragon5"></div>
                    <section class="cloud"></section>
                    <section class="cloud"></section>
                    <section class="cloud"></section>
                </div>
            </div>

            <div class="firecrackers firecrackers-left">

            </div>
            <div class="firecrackers firecrackers-right">

            </div>

        </div>

        <div id="close"></div>

    </div>

    <script>
        window.onload = function(){
            var wantedDragonDance =
                    document.getElementById('dragon-ready-play'); /*开始层*/

            var switcher = document.getElementById('music');/*musci div audio*/
            var media = document.getElementsByTagName('audio')[0];/*audio 标签*/
            var chooseMusic = document.getElementById('music-control');/*静音checkbox*/

            switcher.addEventListener('click',function(){
                var currentStatus = media.paused ? 'pause':'play';

                var wantedStatus = (currentStatus === 'pause'&& !chooseMusic.checked)?'play':'pause';

                media[wantedStatus]();
                // 如果wantedDragonDance 没有is-animationed类名，就添加，反之什么也不做
                if(!wantedDragonDance.classList.contains('is-animationed')){
                    wantedDragonDance.classList.add('is-animationed');
                }

            },false);

            /* 动画结束 */
            wantedDragonDance.addEventListener('webkitAnimationEnd',function(){
                document.querySelector('#dragon-playing').classList.add('is-animationed');
            });
            document.getElementById('close').addEventListener('click',function(){
                var oPopLayer = document.querySelector('.dragon-poplayer');
               oPopLayer.parentNode.removeChild(oPopLayer);

            },false);
            document.querySelector('.firecrackers').addEventListener('webkitAnimationEnd',function(){
              var oPopLayer = document.querySelector('.dragon-poplayer');
               oPopLayer.parentNode.removeChild(oPopLayer);
            },false);
        }
    </script>
